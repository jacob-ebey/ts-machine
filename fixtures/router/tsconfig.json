{
  "compilerOptions": {
    "noEmit": true,

    "target": "ES2020",
    "module": "ESNext",
    "lib": ["DOM", "ES2020"],
    "esModuleInterop": false,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",

    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
